.temp-list-item {
  height: 80rpx;
  padding: 0 30rpx;
  align-items: flex-start;
}
.temp-list-icon {
  width: 45rpx;
  height: 40rpx;
}
.temp-list-box {
  width: 100%;
  position: relative;
  margin-left: 20rpx;
  align-items: center;
  padding-bottom: 20rpx;
  justify-content: space-between;
  border-bottom: 1rpx solid #F9F9F9;
}
.temp-list-title {
  color: #525252;
  font-size: 24rpx;
}
.temp-list-content {
  top: 0rpx;
  right: 50rpx;
  color: #999999;
  font-size: 24rpx;
  position: absolute;
}