/* pages/wifi/index.wxss */
.wifi-warp {
  display: flex;
  height: 100rpx;
  flex-direction: row;
  align-items: center;
  padding: 10rpx 20rpx;
  justify-content: space-between;
}
.wifi {
  font-size: 36rpx;
}
.ssid-wifi-warp {
  width: 500rpx;
  display: flex;
  flex-direction: row;
  align-items: flex-end;
}
.right-wifi {
  width: 40rpx;
  height: 40rpx;
  margin-right: 10rpx;
}
.img-wifi-warp {
  width: 100rpx;
  display: flex;
  flex-direction: row;
  align-items: flex-end;
  justify-content: space-between;
}
.img-wifi {
  width: 50rpx;
  height: 50rpx;
}
.img-lock {
  width: 30rpx;
  height: 30rpx;
}