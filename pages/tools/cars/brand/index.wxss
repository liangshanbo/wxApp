/* pages/tools/cars/brand/index.wxss */
.car-key {
  width: 100%;
  height: 60rpx;
  padding: 0 20rpx;
  align-items: center;
  background-color: #F5F5F5;
}
.car-key-text {
  font-size: 26rpx;
}
.car-list {
  padding: 0 20rpx;
}
.car-item {
  width: 100%;
  height: 140rpx;
  font-size: 30rpx;
  align-items: center;
  border-bottom: 1rpx solid #F5F5F5;
}
.car-logo {
  width: 120rpx;
  height: 80rpx;
  margin-right: 30rpx;
}
.car-price {
  color: #F00;
  font-size: 20rpx;
  padding-top: 6rpx;
}
.car-salestate {
  color: #999;
  font-size: 20rpx;
  padding-top: 6rpx;
}