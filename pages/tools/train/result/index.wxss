/* pages/tools/train/result/index.wxss */
.wrap {
  padding: 40rpx;
}
.line {
  
  margin-bottom: 20rpx;
  padding-bottom: 20rpx;
  flex-direction: column;
  border-bottom: 1rpx solid #EEE;
}
.item {
  display: flex;
  justify-content: space-between;
}
.fm,
.to {
  width: 200rpx;
}
.to {
  align-items: flex-end;
  justify-content: flex-start;
}
.startTime,
.arriveTime {
  color: #609FE2;
}
.arriveTime,
.arrivePoint {
  text-align: right;
}
.durationLine {
  width: 200rpx;
  height: 20rpx;
}
.duration {
  width: 200rpx;
  align-items: center;
}
.durationNo {
  color: #F43530;
}
.site-warp {
  width: 670rpx;
  margin-top: 20rpx;
  justify-content: space-between;
}
.site {
  color: #999;
  font-size: 24rpx;
}